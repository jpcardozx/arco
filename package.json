{
  "name": "arco",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "postbuild": "next-sitemap",
    "build:check": "pnpm typecheck && pnpm lint && pnpm build",
    "build:fast": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "pnpm clean:next && pnpm clean:cache",
    "clean:next": "rimraf .next && rimraf tsconfig.tsbuildinfo",
    "clean:cache": "rimraf .turbo && rimraf node_modules/.cache",
    "clean:deps": "rimraf node_modules && pnpm install",
    "clean:all": "pnpm clean && pnpm clean:deps",
    "prebuild": "pnpm clean:next",
    "test": "bash scripts/run-tests.sh",
    "test:unit": "jest",
    "test:lp": "jest __tests__/lp-smoke.test.ts",
    "test:lp:perf": "node scripts/validate-lp-performance.mjs",
    "test:coverage": "jest --coverage",
    "test:e2e": "npx playwright test",
    "db:status": "npx supabase status",
    "db:push": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "db:migrations": "npx supabase migration list",
    "db:types": "npx supabase gen types typescript --project-id vkclegvrqprevcdgosan > src/types/database.types.ts && echo '✅ Database types regenerated!'",
    "db:verify": "npx supabase db execute --file supabase/verify_payment_system.sql",
    "db:studio": "npx supabase studio",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:link": "npx supabase link --project-ref vkclegvrqprevcdgosan",
    "deploy": "bash scripts/deploy-backend.sh",
    "context:query": "npx tsx src/lib/context-tester.ts query",
    "context:test": "npx tsx src/lib/context-tester.ts test",
    "daily:plan": "npx tsx scripts/daily-planning.ts",
    "mcp:server": "npx tsx src/mcp/servers/arco-unified-intelligence-server.ts",
    "mcp:test": "npx tsx src/mcp/clients/arco-intelligence-tester.ts",
    "mcp:health": "npx tsx src/mcp/scripts/health-check.ts",
    "qa": "npx tsx scripts/qa-automation.ts",
    "qa:build": "npx tsx scripts/qa-automation.ts build",
    "qa:performance": "npx tsx scripts/qa-automation.ts performance",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "bundle:analyze": "pnpm build && npx bundle-analyzer .next/static/chunks/*.js",
    "images:optimize": "node scripts/image-optimizer.cjs",
    "postinstall": "echo '✅ Dependencies installed. Run: pnpm test to verify system'"
  },
